pipeline {
  agent any

  stages {
    stage('Generate Random CSV') {
      steps {
        sh '''
          set -e
          printf "number\n" > random_numbers.csv
          printf "10\n14" >> random_numbers.csv
        '''
        archiveArtifacts artifacts: 'random_numbers.csv', onlyIfSuccessful: true
      }
    }
  }
}
